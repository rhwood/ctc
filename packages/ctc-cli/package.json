{
  "name": "ctc-cli",
  "description": "Control a Centralized Traffic Controller (CTC) Server from the command line",
  "version": "0.0.0",
  "author": "Randall Wood @rhwood",
  "bin": {
    "ctc": "./bin/run"
  },
  "bugs": "https://github.com/rhwood/ctc-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.4.36",
    "@oclif/config": "^1.6.33",
    "@oclif/plugin-help": "^2.0.5",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.15.4",
    "@oclif/test": "^1.1.0",
    "@oclif/tslint": "^1.1.2",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.8",
    "chai": "^4.1.2",
    "globby": "^8.0.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/rhwood/ctc-cli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ctc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "rhwood/ctc-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
