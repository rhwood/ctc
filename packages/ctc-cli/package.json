{
  "name": "ctc",
  "description": "Centralized Traffic Control for Model Railroads",
  "version": "0.0.0",
  "author": "Randall Wood @rhwood",
  "bin": {
    "ctc": "./bin/run"
  },
  "bugs": "https://github.com/rhwood/ctc/issues",
  "dependencies": {
    "@oclif/command": "^1.5.10",
    "@oclif/config": "^1.12.6",
    "@oclif/parser": "^3.7.0",
    "@oclif/plugin-autocomplete": "^0.1.0",
    "@oclif/plugin-help": "^2.1.3",
    "@oclif/plugin-not-found": "^1.2.2",
    "@types/fs-extra": "^5.0.5",
    "@types/json-socket": "^0.1.17",
    "@types/node-ipc": "^9.1.1",
    "@types/npmlog": "^4.1.1",
    "cli-ux": "^5.1.0",
    "fs-extra": "^7.0.1",
    "json-socket": "^0.3.0",
    "npmlog": "^4.1.2",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.2",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.4",
    "chai": "^4.2.0",
    "globby": "^9.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "wait-until": "0.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/rhwood/ctc",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ctc",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "repository": "rhwood/ctc",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "coverage": "npm test && nyc report --reporter html",
    "ci-coverage": "npm test && nyc report --reporter lcov",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
