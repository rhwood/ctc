{
  "name": "ctc",
  "description": "Centralized Traffic Control for Model Railroads",
  "version": "0.0.0",
  "author": "Randall Wood @rhwood",
  "bin": {
    "ctc": "./bin/run"
  },
  "bugs": "https://github.com/rhwood/ctc/issues",
  "dependencies": {
    "@oclif/command": "1.5.19",
    "@oclif/config": "1.13.3",
    "@oclif/parser": "3.8.4",
    "@oclif/plugin-autocomplete": "0.1.4",
    "@oclif/plugin-help": "2.2.1",
    "@oclif/plugin-not-found": "1.2.3",
    "@types/fs-extra": "8.0.1",
    "@types/json-socket": "0.1.17",
    "@types/node-ipc": "9.1.2",
    "@types/npmlog": "4.1.2",
    "cli-ux": "5.3.3",
    "fs-extra": "8.1.0",
    "json-socket": "0.3.0",
    "npmlog": "4.1.2",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@oclif/test": "1.2.5",
    "@oclif/tslint": "3.1.1",
    "@types/chai": "4.2.5",
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.14",
    "chai": "4.2.0",
    "globby": "10.0.1",
    "mocha": "6.2.2",
    "nyc": "14.1.1",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "typescript": "3.7.2",
    "wait-until": "0.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/rhwood/ctc",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ctc",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins"
    ],
    "scope": "rhwood"
  },
  "repository": "rhwood/ctc",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "coverage": "npm test && nyc report --reporter html",
    "ci-coverage": "npm test && nyc report --reporter lcov",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
